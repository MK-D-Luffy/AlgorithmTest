<keymap version="1" name="Windows copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="ctrl f11" />
  </action>
  <action id="CloseAllEditorsButActive">
    <keyboard-shortcut first-keystroke="ctrl numpad1" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="shift ctrl f11" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="Run">
    <keyboard-shortcut first-keystroke="f11" />
  </action>
  <action id="ShowBookmarks">
    <keyboard-shortcut first-keystroke="shift f10" />
  </action>
  <action id="ToggleBookmark">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="ToggleBookmarkWithMnemonic">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="UpdateRunningApplication" />
</keymap>